{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "outDir": "./webpack_bundles/",
  },
  "files": [
    "assets/js/index.tsx"
  ],
  "references": [
    {
      "path": "./js-packages/client/tsconfig.json"
    },
    {
      "path": "./js-packages/misc/tsconfig.json"
    },
    {
      "path": "./js-packages/rdf/tsconfig.json"
    }
  ],
  "paths": {
    "@secretgraph/client": [
      "./js-packages/client/src/*"
    ],
    "@secretgraph/misc": [
      "./js-packages/misc/src/*"
    ],
    "@secretgraph/rdf": [
      "./js-packages/rdf/src/*"
    ]
  },
  "exclude": [
    "node_modules",
    "webpack.config.*.js",
    "**/*.spec.ts"
  ]
  
}
