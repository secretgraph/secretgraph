    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "module": "esnext",
        "importHelpers": true,
        "moduleResolution": "node",
        "lib": [
            "ESNext",
            "dom"
        ]
    },
