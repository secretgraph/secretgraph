{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "target": "ES2017",
        "moduleResolution": "node",
    },
}
